{
  "name": "mongoosastic-ts",
  "version": "5.0.7",
  "description": "A mongoose plugin that indexes models into elastic search",
  "repository": {
    "type": "git",
    "url": "git://github.com/meabed/mongoosastic-ts"
  },
  "license": "MIT",
  "author": "Mohamed Meabed <mo.meabed@gmail.com> (https://meabed.com)",
  "main": "dist/mongoosastic.js",
  "types": "dist/mongoosastic.d.ts",
  "scripts": {
    "authors": "./scripts/update_authors.sh",
    "build": "rm -rf dist && tsc -p . --skipLibCheck",
    "changelog": "changelog mongoosastic/mongoosastic latest -m",
    "coverage": "cat ./coverage/lcov.info | coveralls",
    "lint": "eslint lib test",
    "lint-fix": "yarn lint -- --fix",
    "mocha": "nyc mocha --report lcovonly -- test/*-test.ts",
    "prepare": "yarn husky install",
    "prettier": "prettier --write \"lib/**/*.{ts,tsx,js,jsx}\" \"test/**/*.{ts,tsx,js,jsx}\"",
    "test": "TS_NODE_TRANSPILE_ONLY=1 TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' mocha",
    "test-ci": "TS_NODE_TRANSPILE_ONLY=1 TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' mocha"
  },
  "lint-staged": {
    "lib/*.{ts,tsx,js,jsx}": "prettier --write",
    "test/*.{ts,tsx,js,jsx}": "prettier --write",
    "docs/**": "prettier --write",
    "examples/**": "prettier --write"
  },
  "dependencies": {
    "elasticsearch": "16.7.3",
    "lodash.clonedeep": "4.5.0"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "3.2.0",
    "@types/chai": "4.3.1",
    "@types/elasticsearch": "5.0.40",
    "@types/eslint": "8.4.1",
    "@types/eslint-plugin-prettier": "3.1.0",
    "@types/expect": "24.3.0",
    "@types/lodash.clonedeep": "4.5.7",
    "@types/mocha": "9.1.1",
    "@types/prettier": "2.6.0",
    "@types/should": "13.0.0",
    "@typescript-eslint/eslint-plugin": "5.20.0",
    "@typescript-eslint/parser": "5.20.0",
    "chai": "4.3.6",
    "changelog": "1.4.2",
    "co-mocha": "1.2.2",
    "coveralls": "3.1.1",
    "eslint": "8.14.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-promise": "6.0.0",
    "husky": "7.0.4",
    "lint-staged": "12.4.0",
    "mocha": "9.2.2",
    "mongoose": "6.3.1",
    "nyc": "15.1.0",
    "prettier": "2.6.2",
    "pretty-quick": "3.1.3",
    "should": "13.2.3",
    "standard": "16.0.4",
    "ts-node": "10.7.0",
    "typescript": "4.6.3"
  },
  "engines": {
    "node": ">= 8.0"
  },
  "tags": [
    "mongodb",
    "elasticsearch",
    "elastic search",
    "mongoose",
    "full text search"
  ]
}
